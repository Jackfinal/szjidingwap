/* pages/casepage/casepage.wxss */
.cbanner image{
  width: 100%!important;
    height: 628rpx;
    border: none;
}


.service {
	width: 100%;
	overflow: hidden
}

.service_title {
	width: 100%;
	height: .45rem;
	overflow: hidden;
	border-bottom: 1px solid #e6e6e6;
	text-align: center;
	font-family: AGENCYB_0,ruenzhi;
	font-size: 20rpx;
	line-height: .52rem
}

.service_list {
	width: 100%;
	overflow: auto
}

.service_list dl {
	width: 100%;
	overflow: hidden;
	border-bottom: 1px solid #e6e6e6;
	padding: .1rem 0
}

.service_list dl dt {
	width: .66rem;
	overflow: hidden;
	text-align: center;
	font-size: .14rem;
	color: #333;
	float: left
}

.service_list dl dt img {
	width: .42rem;
	margin-top: .03rem
}

.service_list dl dd {
	overflow: hidden;
	font-size: .12rem;
	color: #888;
	padding-right: .05rem
}

.service_list dl dd span {
	font-size: .14rem;
	color: #333;
	display: block
}
